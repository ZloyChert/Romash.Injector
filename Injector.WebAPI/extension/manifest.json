{
    "manifest_version": 2,
    "name": "Injector",
    "version": "1.0",
    "background": {
        "scripts": [
           "js/background.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Injector",
        "default_popup": "popup.html"
    },
  "permissions": [
    "https://vk.com/im/*",
    "tabs",
    "storage",
    "activeTab"
  ]
}